<div class="card" style="width: 33.333%;">
      <img class="card-img-top" src={{this.image_url}} alt="">
      <div class="card-body">
        <h3 class="card-title">{{this.name}}</h3>
        <h5 class="card-subtitle">{{this.tagline}}</h5>
        <p class="card-text">{{this.description}}</p>
      </div>
</div>